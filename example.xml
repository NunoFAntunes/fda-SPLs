         <component>
            <section>
               <id root="4dd20d3c-b8c1-4026-819d-ff97cc6b3098"/>
               <code code="34068-7" codeSystem="2.16.840.1.113883.6.1" displayName="DOSAGE &amp; ADMINISTRATION SECTION"/>
               <title>Directions</title>
               <text>
                  <list>
                     <item>children 6 years of age and older: chew 1 tablet thoroughly every 6 to 8 hours</item>
                     <item>do not exceed 3 tablets in 24 hours or as directed by a doctor</item>
                     <item>dosage should be taken up to one hour before travel starts or at onset of symptoms</item>
                  </list>
               </text>
               <effectiveTime value="20090617"/>
            </section>
         </component>
