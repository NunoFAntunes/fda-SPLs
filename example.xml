               <code code="34067-9" codeSystem="2.16.840.1.113883.6.1" displayName="INDICATIONS &amp; USAGE SECTION"/>
               <title>Uses</title>
               <text>
                  <paragraph>prevents and treats nausea, vomiting or dizziness associated with motion sickness</paragraph>
               </text>
               <effectiveTime value="20090617"/>
            </section>
